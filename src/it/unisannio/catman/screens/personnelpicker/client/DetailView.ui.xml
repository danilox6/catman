<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:c="urn:import:it.unisannio.catman.common.client.ui"
	xmlns:my="urn:import:it.unisannio.catman.screens.personnelpicker.client.widgets"
	xmlns:ct="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		
	</ui:style>

	<c:MasterPanel ui:field="masterPanel">
		<c:header>
			<c:ActionBar>
				<c:left>
					<b:Heading size="3" ui:field="titleLabel"></b:Heading>
				</c:left>
				<c:right>
					<b:ButtonGroup>
						<b:Button icon="REMOVE_SIGN" type="DANGER" />
						<b:Button ui:field="addButton" icon="PLUS" type="PRIMARY">Aggiungi</b:Button>
					</b:ButtonGroup>
				</c:right>
			</c:ActionBar>
		</c:header>
		<c:content>
			<b:Fieldset>
				<b:Legend>Selezionati</b:Legend>
				<my:SelectedWorkersCellList ui:field="selectedCellList"></my:SelectedWorkersCellList>
			</b:Fieldset>
		</c:content>
		<c:content>
			<b:Fieldset>
				<b:Legend>Disponibili</b:Legend>
				<ct:CellTree ui:field="cellTree" />
			</b:Fieldset>
		</c:content>
	</c:MasterPanel>
</ui:UiBinder>  