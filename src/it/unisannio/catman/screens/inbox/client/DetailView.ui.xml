	<ui:style>
		.background {
			background-color: #99C;
		}
	</ui:style>

	<g:DockPanel width="100%" ui:field="dock" >
		<g:Dock direction="NORTH" >
			<g:SimplePanel ui:field="northPanel">
				<cm:HeadWidget></cm:HeadWidget>
			</g:SimplePanel>
		</g:Dock>
		<g:Dock direction="CENTER" width="100%">
			<g:ScrollPanel width="100%" ui:field="scroll">
				<g:VerticalPanel width="100%">
					<my:ContactDetailItemWidget></my:ContactDetailItemWidget>
					<my:DocumentDetailItemWidget></my:DocumentDetailItemWidget>
					<my:RequirementDetailItemWidget></my:RequirementDetailItemWidget>
					<my:ProviderDetailItemWidget></my:ProviderDetailItemWidget>
					<my:ProfileCardWidget></my:ProfileCardWidget>
				</g:VerticalPanel>
			</g:ScrollPanel>
		</g:Dock>
		<g:Dock direction="SOUTH">
			<g:SimplePanel ui:field="southPanel" >
				
			</g:SimplePanel>
		</g:Dock>
	</g:DockPanel>
</ui:UiBinder> 