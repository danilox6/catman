<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:VerticalPanel width="113px">
		<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_TOP">
			<g:Button height="15px" width="15px" ui:field="closeButton"></g:Button>
		</g:cell>
		<g:cell horizontalAlignment="CENTER">
			<g:SimplePanel ui:field="picturePanel">
				<g:Button height="77px" width="79px" text="foto"></g:Button>
			</g:SimplePanel>
		</g:cell>
		<g:cell horizontalAlignment="CENTER">
			<g:Label ui:field="nameLabel">John Smith</g:Label>
		</g:cell>
		<g:cell horizontalAlignment="CENTER">
			<g:Label ui:field="roleLabel">Waiter</g:Label>
		</g:cell>
	</g:VerticalPanel>
</ui:UiBinder> 