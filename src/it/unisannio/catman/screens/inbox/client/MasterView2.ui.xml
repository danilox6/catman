<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:c="urn:import:it.unisannio.catman.common.client.ui">
	<c:MasterPanel>
	    <c:header>
	        <g:Label>Title</g:Label>
	    </c:header>
	    <c:content>
	        <c:DataList ui:field="dataList" />
	    </c:content>
	    <c:footer>
	        <c:ActionBar>
	            <c:left>
	                <g:Button ui:field="makeNew">New</g:Button>
	            </c:left>
	            
	            <c:right>
	                <g:Button>Select</g:Button>
	            </c:right>
	        </c:ActionBar>
	    </c:footer>
	</c:MasterPanel>
</ui:UiBinder> 